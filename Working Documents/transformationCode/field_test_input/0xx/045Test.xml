<?xml version="1.0" encoding="UTF-8"?>
<marc:collection xmlns:marc="http://www.loc.gov/MARC21/slim"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.loc.gov/MARC21/slim http://www.loc.gov/standards/marcxml/schema/MARC21slim.xsd">
    
    <marc:record>
        <marc:leader>01234cam a2200301 i 4500</marc:leader>
        
        <!-- Example 1: Time period code -->
        <marc:datafield tag="045" ind1=" " ind2=" ">
            <marc:subfield code="a">t-t-</marc:subfield>
        </marc:datafield>
        
        <!-- Example 2: Formatted CE date in $b -->
        <marc:datafield tag="045" ind1="0" ind2=" ">
            <marc:subfield code="b">d17910304</marc:subfield>
        </marc:datafield>
        
        <!-- Example 3: BCE range in $c -->
        <marc:datafield tag="045" ind1="2" ind2=" ">
            <marc:subfield code="c">225000000</marc:subfield>
            <marc:subfield code="c">70000000</marc:subfield>
        </marc:datafield>
        
        <!-- Example 4: Multiple $b -->
        <marc:datafield tag="045" ind1="1" ind2=" ">
            <marc:subfield code="b">d1972</marc:subfield>
        </marc:datafield>
        
        <!-- Example 5: Mixed a/b/c -->
        <marc:datafield tag="045" ind1="2" ind2=" ">
            <marc:subfield code="b">d1984</marc:subfield>
            <marc:subfield code="c">100000000</marc:subfield>
        </marc:datafield>
        
    </marc:record>
</marc:collection>
